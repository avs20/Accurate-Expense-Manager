<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="siolabs.osahub" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="siolabs.osahub.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="siolabs.osahub.Activity.ViewAccount"
            android:label="@string/title_activity_view_account" >
        </activity>
        <activity
            android:name="siolabs.osahub.Activity.ViewCategory"
            android:label="@string/title_activity_view_account" >
        </activity>
        <activity
            android:name="siolabs.osahub.Activity.ViewAllTransaction"
            android:label="@string/title_activity_view_all_transaction" >
        </activity>
        <activity
            android:name="siolabs.osahub.Activity.AddNewTransactionActivity"
            android:label="@string/title_activity_add_new_transaction" >
        </activity>
    </application>

</manifest>
